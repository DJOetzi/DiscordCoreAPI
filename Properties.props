		  xcopy "$(ProjectDir)Library Files\$(Configuration)\libsodium.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\opus.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\avcodec-58.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\swresample-3.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\avformat-58.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
