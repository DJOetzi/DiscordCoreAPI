		  xcopy "$(ProjectDir)Library Files\$(Configuration)\libcurl.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\libcurl-d.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\zlibd1.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\pcred.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\intl-8.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
		  xcopy "$(ProjectDir)Library Files\$(Configuration)\iconv-2.dll" "$(SolutionDir)$(Platform)\$(Configuration)" /y /e
	  </Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>